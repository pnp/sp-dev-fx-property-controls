    "@pnp/sp-taxonomy": "1.3.11",
    "@pnp/telemetry-js": "2.0.0",
    "@uifabric/icons": "7.5.17",
    "lodash.omit": "4.5.0",
    "markdown-to-jsx": "^6.11.4",
    "office-ui-fabric-react": "7.156.0",
    "react": "16.9.0",
    "react-ace": "5.8.0",
